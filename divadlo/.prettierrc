{
    "arrowParens": "always",
    "bracketSameLine": false,
    "htmlWhitespaceSensitivity": "css",
    "jsxSingleQuote": false,
    "printWidth": 80,
    "semi": true,
    "proseWrap": "preserve",
    "quoteProps": "consistent",
    "singleQuote": false,
    "tabWidth": 4,
    "trailingComma": "all",
    "useTabs": false,
    "plugins": ["prettier-plugin-tailwindcss"]
}
